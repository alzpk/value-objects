{
  "name": "alzpk/value-objects",
  "description": "A set of value objects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "alzpk",
      "email": "mh1986@outlook.dk"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Alzpk\\ValueObjets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alzpk\\ValueObjets\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
